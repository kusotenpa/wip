{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "plugins": [
    "react",
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
  ],
  "settings": {
    "react": {
      "pragma": "h",
    },
  },
  "rules": {
    "arrow-parens": [2, "as-needed"],
    "array-bracket-spacing": [2, "always"],
    "comma-dangle": [2, "always-multiline"],
    "comma-spacing": [2, {"before": false, "after": true}],
    "computed-property-spacing": [2, "always"],
    "prefer-const": 2,
    "eqeqeq": [2, "always"],
    "no-lonely-if": 2,
    "no-multi-spaces": 2,
    "no-multiple-empty-lines": [2, {"max": 1}],
    "no-param-reassign": 2,
    "no-self-compare": 2,
    "no-shadow": 2,
    "no-var": 2,
    "no-debugger": 1,
    "no-console": 1,
    "no-tabs": 2,
    "object-curly-spacing": [2, "always"],
    "quotes": [2, "single"],
    "new-cap": 2,
    "semi": [2, "never"],
    "space-in-parens": 2,
    "space-before-function-paren": [2, "never"],
    "space-before-blocks": 2,
    "space-infix-ops": 2,
    "func-call-spacing": 2,
    "keyword-spacing": 2,
    "indent": [2, 2],

    "react/prop-types": 0,
  },
}
